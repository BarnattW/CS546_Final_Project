<h1>Browse Listings</h1>
<div class="listings-container">
    {{#each listings}}
        <div class="listing-item">
            <img src="{{image}}" alt="{{title}}" class="listing-image">
            <h2>{{title}}</h2>
            <p>{{description}}</p>
            <p><strong>Price: ${{price}}</strong></p>
            <button class="btn-add-to-cart">Add to Cart</button>
        </div>
    {{/each}}
</div>
